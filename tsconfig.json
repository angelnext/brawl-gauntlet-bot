{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "noEmit": true,
    "module": "nodenext",
    "target": "es2017",
    "strict": true,
    "lib": [
      "ES2023"
    ]
  },
  "include": [
    "."
  ],
  "exclude": [
    "node_modules"
  ]
}
